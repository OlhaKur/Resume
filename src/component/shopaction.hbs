<section>
  {{#with service}}
    <div class='card'>
      <div class='card-body text-center text-bg-success'>
        <h2>{{title}}</h2>
        <p>{{description}}</p>
        {{#each buttons}}
          <a
            href='{{this.link}}'
            class='btn btn-primary'
          >{{text}}</a>
        {{/each}}
      </div>
    </div>
  {{/with}}
</section>