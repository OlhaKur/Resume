<main>
  <div class='card'>
    {{#with heading}}
      <div class='card-body'>
        <h1>{{main.title}}</h1>
        <p>{{main.text}}</p>
        <button
          class='btn btn-primary'
        >{{main.button_text.solid}}</button>
        <button
          class='btn btn-outline-primary'
        >{{main.button_text.outline}}</button>
      </div>
    {{/with}}
  </div>
  <br />
  <div class='card'>
    {{#with heading}}
      <div class='card-body'>
        <h2>{{second.title}}</h2>
        <p>{{second.text}}</p>
        <button
          class='btn btn-secondary'
        >{{second.button_text.solid}}</button>
        <button
          class='btn btn-outline-secondary'
        >{{second.button_text.outline}}</button>
      </div>
    {{/with}}
  </div>
  <br />
  <div class='card'>
    {{#with heading}}
      <div class='card-body'>
        <h3>{{subblock.title}}</h3>
        <p>{{subblock.text}}</p>
        <button
          class='btn btn-dark'
        >{{subblock.button_text.solid}}</button>
        <button
          class='btn btn-outline-dark'
        >{{subblock.button_text.outline}}</button>
      </div>
    {{/with}}
  </div>
  <br />
  <br />
  <div class='card card-body'>
    {{#with nested_display}}
      <div class='display-1'>{{title}}</div>
      <div class='card'>
        <div class='card-body'>
          <div
            class='display-4'
          >{{children_display.title}}</div>
          <p>{{children_display.text}}</p>

          <button
            class='btn btn-outline-warning'
          >{{children_display.button.text}}</button>
        </div>
      </div>
      <br />
      <div>
        <h6>{{header_text}}</h6>
        <button
          class='btn btn-info text-light'
        >{{button.text}}</button>
      </div>
    {{/with}}
  </div>

  <br />
  <br />
  <br />
  <div class='card card-body'>
    {{#with paragraph}}

      <div class='text-uppercase'>{{title}}</div>
      <p>{{text}}</p>
      <a href='url' class='btn btn-link'>{{button.text}}</a>
      <div class='card card-body'>
        {{#each list_name}}
          <ul>
            <li>
              <a href='url'>{{text}}</a>
            </li>
          </ul>
        {{/each}}
      </div>

    {{/with}}
  </div>
</main>